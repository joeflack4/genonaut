{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:debug": "VITE_DEBUG_GENERATION=true vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test-unit": "vitest run",
    "test": "npm run test-unit && npm run test:e2e",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"src/**/*.{ts,tsx}\"",
    "openapi:types": "node scripts/generate-openapi.mjs",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "DEBUG_E2E=true playwright test",
    "test:e2e:debug:headed": "DEBUG_E2E=true playwright test --headed",
    "test:e2e:non-performance": "playwright test --grep-invert '@performance'",
    "test:e2e:non-performance:headed": "playwright test --headed --grep-invert '@performance'",
    "test:e2e:non-performance:ui": "playwright test --ui --grep-invert '@performance'",
    "test:e2e:non-performance:debug": "DEBUG_E2E=true playwright test --grep-invert '@performance'",
    "test:e2e:non-performance:debug:headed": "DEBUG_E2E=true playwright test --headed --grep-invert '@performance'",
    "test:e2e:performance": "playwright test --config=playwright-performance.config.ts --grep '@performance'",
    "test:e2e:performance:headed": "playwright test --config=playwright-performance.config.ts --headed --grep '@performance'",
    "test:e2e:performance:ui": "playwright test --config=playwright-performance.config.ts --ui --grep '@performance'",
    "test:e2e:performance:debug": "DEBUG_E2E=true playwright test --config=playwright-performance.config.ts --grep '@performance'",
    "test:e2e:performance:debug:headed": "DEBUG_E2E=true playwright test --config=playwright-performance.config.ts --headed --grep '@performance'",
    "test:e2e:real-api": "playwright test --config=playwright-real-api.config.ts",
    "test:e2e:real-api:headed": "playwright test --config=playwright-real-api.config.ts --headed",
    "test:e2e:real-api:ui": "playwright test --config=playwright-real-api.config.ts --ui",
    "test:e2e:real-api:debug": "DEBUG_E2E=true playwright test --config=playwright-real-api.config.ts",
    "test:e2e:real-api:debug:headed": "DEBUG_E2E=true playwright test --config=playwright-real-api.config.ts --headed",
    "test:api-server": "node scripts/test-api-server.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "react": "^19.1.1",
    "react-accessible-treeview": "^2.11.2",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.3.0"
  },
  "devDependencies": {
    "@esbuild/darwin-arm64": "^0.25.10",
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@rollup/rollup-darwin-arm64": "^4.52.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.10",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.8.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.6.2",
    "rollup": "^4.43.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}
